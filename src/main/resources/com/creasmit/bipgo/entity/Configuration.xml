<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.creasmit.bipgo.entity.Client" table="Configuration" catalog="bipGo" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="pourcentageRetenu" type="java.lang.Integer">
            <column name="pourcentageRetenu" length="19" />
        </property>
        <property name="pourcentageComission" type="java.lang.Integer">
            <column name="pourcentageComission" length="19" />
        </property>
    </class>
</hibernate-mapping>

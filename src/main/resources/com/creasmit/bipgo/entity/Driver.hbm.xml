<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.creasmit.bipgo.entity.Driver" table="driver">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="modePaiement" type="string">
            <column name="modePaiement" />
        </property>
        <property name="fkStructure" type="java.lang.Integer">
            <column name="fkStructure" />
        </property>
        <property name="fkIdentite" type="java.lang.Integer">
            <column name="fkIdentite" />
        </property>
        <property name="fkVehicule" type="java.lang.Integer">
            <column name="fkVehicule"/>
        </property>
        <property name="fkWallet" type="java.lang.Integer">
            <column name="fkWallet"/>
        </property>
        <property name="dateCreate" type="timestamp">
            <column name="dateCreate" />
        </property>
        <property name="dateModif" type="timestamp">
            <column name="dateModif" />
        </property>
        <property name="agentCreate" type="java.lang.Integer">
            <column name="agentCreate" />
        </property>
        <property name="agentModif" type="java.lang.Integer">
            <column name="agentModif" />
        </property>
        <property name="adresse" type="string">
            <column name="adresse" />
        </property>
    </class>
</hibernate-mapping>
